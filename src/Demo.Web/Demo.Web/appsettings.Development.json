{
  "Diagnosis": {
    "SlowSql": {
      "Enabled": true,
      "ThresholdMs": 200,
      "LogOnError": true,
      "IncludeTrace": true,
      "IncludeDatabase": true,
      "IncludeParameters": false,
      "SensitiveParameterNames": [ "password", "pwd", "token", "secret" ],
      "MaxParameterValueLength": 256,
      "MaxCommandTextLength": 4096
    },
    "AuditLog": {
      "Enabled": true,
      "WriterMode": "Database",
      "IgnoreWriteErrors": true
    }
  }
}
